<?xml version="1.0" encoding="UTF-8"?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="cache-warmup">
        <argument name="data" xsi:type="array">
            <item name="config" xsi:type="array">
                <item name="collapsible" xsi:type="boolean">true</item>
                <item name="label" xsi:type="string" translate="true">Warm up page cache</item>
                <item name="additionalClasses" xsi:type="string">cs-csfeature__logo</item>
                <item name="notice" xsi:type="string" translate="true">
                    Page cache for this product can be warmed-up. Select if it shall be warmed up and how important the warm-up is.
                </item>
            </item>
        </argument>
    </fieldset>
</form>
